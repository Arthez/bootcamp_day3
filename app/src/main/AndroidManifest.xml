<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.active.maka.activeandroidapplication" >

    <meta-data
        android:name="AA_MODELS"
        android:value="com.active.maka.activeandroidapplication.database.model.Pokemon,
                        com.active.maka.activeandroidapplication.database.model.Coach
                         com.active.maka.activeandroidapplication.database.model.CoachPokemonRelation"/>

    <application
        android:name=".ActiveAndroidApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".ui.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
